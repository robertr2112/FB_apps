<% provide(:title, 'Create entry') %>
<h1>Create Entry</h1>
<div class="row">
  <div class="span6 offset2">
    <%= form_for([@pool, @entry], html: { class: 'form-horizontal' }) do |f| %>
      <%= render 'fields', f: f %>
      <div class="control-group">
        <div class = "controls">
          <%= f.submit "Create entry", class: "btn btn-large btn-primary" %>
        </div>
      </div>
    <% end %>
  </div>
</div>
